{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "encoding": true,
    "indent": [true, "spaces", 2],
    "no-console": [false]
  },
  "rulesDirectory": []
}
